{
    "name": "hughnoble/injector",
    "description": "An easy to configure and flexible PSR-11 compliant IoC container for PHP",
    "type": "library",
    "keywords": ["IoC", "Container", "Dependency", "Injection"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hugh Noble",
            "email": "contact@hughnoble.co.uk"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Injector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Injector\\Test\\": "test/"
        }
    }
}
